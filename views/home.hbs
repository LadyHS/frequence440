<section id="home">
  <header class="header-intro main-section">
    <div class="main-title">
      <h1><span>{{{__ "home.mainTitle"}}}</span></h1>
    </div>
    <p>{{{__ "home.introText"}}}</p>
  </header>
  <section class="discover main-section">
    <div class="left">
      <div class="prg-header">
        <h2>{{{__ "home.welcomeMessage"}}}</h2>
        <p>{{{__ "home.welcomeSentence"}}}.
        </p>
      </div>
    </div>
    <div class="right">
      <div class="right-top">
        <div class="right-main">
          <ul class="inline-list">
            <li>{{{__ "home.rightTop.details.1"}}}</li>
            <li>{{{__ "home.rightTop.details.2"}}}</li>
            <li>{{{__ "home.rightTop.details.3"}}}</li>
          </ul>
          <h2>{{{__ "home.rightTop.mainGoal"}}}</h2>
        </div>
        <div class="right-footer">
          <h3 class="semi-underlined">
            <a href="/prestations">{{{__ "home.rightTop.link"}}}</a>
          </h3>
        </div>
      </div>
      <div class="right-bottom">
        <div class="right-main">
          <ul class="inline-list">
            <li>{{{__ "home.rightBottom.details.1"}}}</li>
            <li>{{{__ "home.rightBottom.details.2"}}}</li>
            <li>{{{__ "home.rightBottom.details.3"}}}</li>
            <li>{{{__ "home.rightBottom.details.4"}}}</li>
          </ul>
          <h2>{{{__ "home.rightBottom.mainGoal"}}}</h2>
        </div>
        <div class="right-footer">
          <h3 class="semi-underlined">
            <a href="/ressources">{{{__ "home.rightBottom.link"}}}</a>
          </h3>
        </div>
      </div>
    </div>
  </section>
  <section class="clients main-section">
    <div class="prg-header">
      <h2>{{{__ "home.clients.title"}}}</h2>
      <p>{{{__ "home.clients.sentence"}}}</p>
    </div>
    <div class="grid">
      <figure class="img-cell"> <img src="/images/cerdd.png" alt="logo CERDD">
      </figure>
      <figure class="img-cell"> <img src="/images/ldv.png" alt="logo Leonard de Vinci">
      </figure>
      <figure class="img-cell"> <img src="/images/gw.png" alt="logo Greenworking">
      </figure>
      <figure class="img-cell"> <img src="/images/framatome.png" alt="logo framatome">
      </figure>
      <figure class="img-cell"> <img src="/images/ft.png" alt="logo France Télévisions">
      </figure>
      <figure class="img-cell"> <img src="/images/sg.png" alt="logo Société Générale">
      </figure>
      <figure class="img-cell"> <img src="/images/mr21.png" alt="logo MR21">
      </figure>
      <figure class="img-cell"> <img src="/images/centrale.png" alt="logo Centrale Supélec">
      </figure>
    </div>
  </section>
  <section class="testimonies main-section">
    <blockquote class="slides">
      <p>{{{__ "home.quotes.1.sentence"}}}</p>
      <footer>{{{__ "home.quotes.1.user"}}}</footer>
    </blockquote>
    <blockquote class="slides">
      <p>{{{__ "home.quotes.2.sentence"}}}</p>
      <footer>{{{__ "home.quotes.2.user"}}}</footer>
    </blockquote>
    <div class="dots">
      <span class="dot" data-index="0"></span>
      <span class="dot" data-index="1"></span>
    </div>
  </section>
</section>