    </div>
    <div class="languages">
      <a href="/fr">FR</a> |
      <a href="/en">EN</a>
    </div>
  </nav>
  <nav id="navbar-fullscreen" class="flex-col" data-page="{{currentPage}}">
    <ul class="flex-col space-e">
      <li data-link="home"><a href="{{{__ "nav.rawLinks.1"}}}">{{{__ "nav.1"}}}</a></li>
      <li data-link="transition"><a href="{{{__ "nav.rawLinks.2"}}}">{{{__ "nav.2"}}}</a></li>
